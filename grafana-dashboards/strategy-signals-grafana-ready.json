{
  "id": null,
  "uid": "aibetting-strategy-signals",
  "title": "Strategy Signals Monitor",
  "tags": ["aibetting", "analyst", "signals", "pro-strategies"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"],
    "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d"]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "title": "Total Signals Today",
      "type": "stat",
      "datasource": {
        "type": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 8,
        "h": 4
      },
      "targets": [
        {
          "expr": "sum(aibetting_analyst_signals_by_type_total)",
          "refId": "A",
          "legendFormat": "Total Signals"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 10,
                "color": "yellow"
              },
              {
                "value": 50,
                "color": "green"
              }
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 2,
      "title": "High-Confidence Signals",
      "type": "stat",
      "datasource": {
        "type": "prometheus"
      },
      "gridPos": {
        "x": 8,
        "y": 0,
        "w": 8,
        "h": 4
      },
      "targets": [
        {
          "expr": "count(aibetting_analyst_last_signal_confidence > 0.8)",
          "refId": "A",
          "legendFormat": "High Confidence (>80%)"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 5,
                "color": "yellow"
              },
              {
                "value": 20,
                "color": "green"
              }
            ]
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 3,
      "title": "Average ROI (All Strategies)",
      "type": "stat",
      "datasource": {
        "type": "prometheus"
      },
      "gridPos": {
        "x": 16,
        "y": 0,
        "w": 8,
        "h": 4
      },
      "targets": [
        {
          "expr": "avg(aibetting_analyst_last_signal_roi)",
          "refId": "A",
          "legendFormat": "Avg ROI"
        }
      ],
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "value_and_name",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 3,
                "color": "green"
              }
            ]
          },
          "unit": "percent",
          "decimals": 2
        }
      }
    },
    {
      "id": 4,
      "title": "Strategy Signals Distribution (Last Hour)",
      "type": "piechart",
      "datasource": {
        "type": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum by (strategy) (increase(aibetting_analyst_signals_by_type_total[1h]))",
          "refId": "A",
          "legendFormat": "{{strategy}}"
        }
      ],
      "options": {
        "pieType": "pie",
        "displayLabels": ["name", "percent"],
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "values": ["value", "percent"]
        },
        "tooltip": {
          "mode": "single"
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          },
          "unit": "short"
        }
      }
    },
    {
      "id": 5,
      "title": "Signals by Risk Level (Last Hour)",
      "type": "bargauge",
      "datasource": {
        "type": "prometheus"
      },
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "sum by (risk_level) (increase(aibetting_analyst_signals_by_type_total[1h]))",
          "refId": "A",
          "legendFormat": "{{risk_level}}"
        }
      ],
      "options": {
        "orientation": "horizontal",
        "displayMode": "gradient",
        "showUnfilled": true,
        "text": {
          "valueSize": 24
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 5,
                "color": "yellow"
              },
              {
                "value": 10,
                "color": "orange"
              },
              {
                "value": 15,
                "color": "red"
              }
            ]
          },
          "unit": "short",
          "mappings": [
            {
              "type": "value",
              "options": {
                "Low": {
                  "color": "green",
                  "index": 0
                },
                "Medium": {
                  "color": "yellow",
                  "index": 1
                },
                "High": {
                  "color": "orange",
                  "index": 2
                },
                "VeryHigh": {
                  "color": "red",
                  "index": 3
                }
              }
            }
          ]
        }
      }
    },
    {
      "id": 6,
      "title": "Signal Rate (signals/hour)",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 24,
        "h": 8
      },
      "targets": [
        {
          "expr": "rate(aibetting_analyst_signals_by_type_total[5m]) * 3600",
          "refId": "A",
          "legendFormat": "{{strategy}} - {{signal_type}}"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true,
          "calcs": ["lastNotNull", "max", "mean"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never",
            "spanNulls": true
          },
          "unit": "short",
          "decimals": 1
        }
      }
    },
    {
      "id": 7,
      "title": "Signal Confidence Timeline",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 20,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "aibetting_analyst_last_signal_confidence",
          "refId": "A",
          "legendFormat": "{{strategy}}"
        }
      ],
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true,
          "calcs": ["lastNotNull", "mean"]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 15,
            "showPoints": "auto",
            "spanNulls": true
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 0.6,
                "color": "yellow"
              },
              {
                "value": 0.75,
                "color": "light-green"
              },
              {
                "value": 0.85,
                "color": "green"
              }
            ]
          },
          "unit": "percentunit",
          "min": 0,
          "max": 1
        }
      }
    },
    {
      "id": 8,
      "title": "Expected ROI by Strategy",
      "type": "barchart",
      "datasource": {
        "type": "prometheus"
      },
      "gridPos": {
        "x": 12,
        "y": 20,
        "w": 12,
        "h": 8
      },
      "targets": [
        {
          "expr": "avg by (strategy) (aibetting_analyst_last_signal_roi)",
          "refId": "A",
          "legendFormat": "{{strategy}}"
        }
      ],
      "options": {
        "orientation": "vertical",
        "xTickLabelRotation": 0,
        "xTickLabelSpacing": 0,
        "showValue": "always",
        "legend": {
          "displayMode": "hidden",
          "placement": "bottom",
          "showLegend": false
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "continuous-GrYlRd"
          },
          "custom": {
            "axisCenteredZero": false,
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            }
          },
          "unit": "percent",
          "decimals": 2
        }
      }
    },
    {
      "id": 9,
      "title": "Latest Signals Summary",
      "type": "table",
      "datasource": {
        "type": "prometheus"
      },
      "gridPos": {
        "x": 0,
        "y": 28,
        "w": 24,
        "h": 8
      },
      "targets": [
        {
          "expr": "aibetting_analyst_last_signal_confidence",
          "refId": "A",
          "format": "table",
          "instant": true
        },
        {
          "expr": "aibetting_analyst_last_signal_roi",
          "refId": "B",
          "format": "table",
          "instant": true
        },
        {
          "expr": "sum by (strategy) (aibetting_analyst_signals_by_type_total)",
          "refId": "C",
          "format": "table",
          "instant": true
        },
        {
          "expr": "aibetting_analyst_strategy_avg_confidence",
          "refId": "D",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "instance": true,
              "job": true
            },
            "indexByName": {
              "strategy": 0,
              "Value #A": 1,
              "Value #B": 2,
              "Value #C": 3,
              "Value #D": 4
            },
            "renameByName": {
              "strategy": "Strategy",
              "Value #A": "Last Confidence",
              "Value #B": "Last ROI (%)",
              "Value #C": "Total Signals",
              "Value #D": "Avg Confidence"
            }
          }
        }
      ],
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "displayName": "Total Signals",
            "desc": true
          }
        ]
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "center",
            "cellOptions": {
              "type": "auto"
            },
            "filterable": false,
            "inspect": false
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "text"
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Last Confidence"
            },
            "properties": [
              {
                "id": "unit",
                "value": "percentunit"
              },
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-background"
                }
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "value": null,
                      "color": "red"
                    },
                    {
                      "value": 0.6,
                      "color": "yellow"
                    },
                    {
                      "value": 0.8,
                      "color": "green"
                    }
                  ]
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Last ROI (%)"
            },
            "properties": [
              {
                "id": "unit",
                "value": "percent"
              },
              {
                "id": "decimals",
                "value": 2
              },
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-text"
                }
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "value": null,
                      "color": "text"
                    },
                    {
                      "value": 3,
                      "color": "green"
                    },
                    {
                      "value": 5,
                      "color": "dark-green"
                    }
                  ]
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Avg Confidence"
            },
            "properties": [
              {
                "id": "unit",
                "value": "percentunit"
              },
              {
                "id": "decimals",
                "value": 2
              }
            ]
          }
        ]
      }
    }
  ],
  "templating": {
    "list": []
  },
  "annotations": {
    "list": []
  }
}
